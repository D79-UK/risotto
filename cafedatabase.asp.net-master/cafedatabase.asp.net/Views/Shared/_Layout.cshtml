<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
		<link href="https://fonts.googleapis.com/css?family=Quicksand:400,700%7CCabin:400%7CDancing+Script" rel="stylesheet">
    @Styles.Render("~/Content/css")
    @Styles.Render("~/fonts")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
<!-- Header -->
		<header id="header">

			<!-- Top nav -->
			<div id="top-nav">
				<div class="container">

				<!-- logo -->
				<div class="logo">
					<a href="index.html"><img src="@Url.Content("~/Content/Images/logo.png")"/></a>
				</div>
				<!-- logo -->

				<!-- Mobile toggle -->
				<button class="navbar-toggle">
					<span></span>
				</button>
				<!-- Mobile toggle -->

	

				</div>
			</div>
			<!-- /Top nav -->

			<!-- Bottom nav -->
			<div id="bottom-nav">
				<div class="container">
				<nav id="nav">

					<!-- nav -->
					<ul class="main-nav nav navbar-nav">
                        <li>@Html.ActionLink("Home", "Index", "Home")</li>
                        <li>@Html.ActionLink("About", "About", "Home")</li>
						<li><a href="#menu">Menu</a></li>						
						<li><a href="#cuisine">Cuisine</a></li>
						
                        <li>@Html.ActionLink("Events", "Index", "cevents")</li>
                        <li>@Html.ActionLink("posts", "Index", "posts")</li>
                        <li><a href="#contact">Contact Us</a></li>
					</ul>
					<!-- /nav -->
                    @Html.Partial("_LoginPartial")
					<!-- button nav -->
					<ul class="cta-nav">
						<li><a href="#reservation" class="main-button">Reservation</a></li>
					</ul>
					<!-- button nav -->

		

				</nav>
				</div>
			</div>
			<!-- /Bottom nav -->


		</header>
		<!-- /Header -->


        @RenderBody()

		
		<!-- Footer -->
		<footer id="footer">

			<!-- container -->
			<div class="container">

				<!-- row -->
				<div class="row">

					<!-- copyright -->
					<div class="col-md-6">
						<!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
						<span class="copyright">Copyright 2018 All rights reserved | This template is made with <i class="fa fa-heart-o" aria-hidden="true"></i> by <a href="https://colorlib.com" target="_blank">Colorlib</a></span>
						<!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
					</div>
					<!-- /copyright -->

					<!-- footer nav -->
					<div class="col-md-6">
						<nav class="footer-nav">
                            @Html.ActionLink("Home", "Index", "Home")
                            @Html.ActionLink("About", "About", "Home")
                            <a href="#menu">Menu</a>
                            <a href="#reservation">Reservation</a>
                            <a href="#cuisine">Cuisine</a>
                            <a href="#events">Events</a>
                            @Html.ActionLink("Contact", "Contact", "Home")
						</nav>
					</div>
					<!-- /footer nav -->

				</div>
				<!-- /row -->

			</div>
			<!-- /container -->

		</footer>
		<!-- /Footer -->

		<!-- Preloader -->
		<div id="preloader">
			<div class="preloader">
				<span></span>
				<span></span>
				<span></span>
			</div>
		</div>
		<!-- /Preloader -->

		<!-- jQuery Plugins -->
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/owl.carousel")
		<script src="https://maps.googleapis.com/maps/api/js?v=3.exp&sensor=false"></script>
    @Scripts.Render("~/bundles/google-map")
    @Scripts.Render("~/bundles/main")
    @Scripts.Render("~/bundles/owl.carousel")
    @RenderSection("scripts", required: false)

</body>
</html>
